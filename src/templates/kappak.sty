%% --------------------

\ifthenelse{\equal{\@titlePageStyle}{classic}}{
    \renewcommand{\maketitle}{
        \begin{titlepage}
            \begin{center}
                \logohere{centeredFigure} ~ \\ [2cm]
                \huge{\textsc{\@title}} \\ [1.5cm]
                \begin{center} \large
                    \authorshere \\ [0.25cm]
                \end{center}
                {\large \@date} \\ [4cm]
